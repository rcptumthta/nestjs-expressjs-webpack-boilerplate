{
  "name": "nestjs-expressjs-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for NestJS (ExpressJS) webpack",
  "scripts": {
    "lint": "eslint . --ext .js,.json,.json5,.jsonc,.jsonl,.ts",
    "lint:fix": "eslint . --ext .js,.json,.json5,.jsonc,.jsonl,.ts --fix",
    "build": "nest build",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit-watch": "jest --watch",
    "test:unit-coverage": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "10.4.7",
    "@nestjs/core": "10.4.7",
    "@nestjs/platform-express": "10.4.7",
    "express": "4.21.1",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.7",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.7",
    "@suites/di.nestjs": "3.0.0",
    "@suites/doubles.jest": "3.0.0",
    "@suites/unit": "3.0.0",
    "@types/eslint": "8.56.10",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsonc": "2.17.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "jest": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "private": true,
  "engines": {
    "node": "^20"
  }
}
